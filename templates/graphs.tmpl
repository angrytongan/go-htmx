{{block "graphs-page" .}}
  {{template "head" .}}
  {{template "nav" .}}

  {{template "graphs" .}}

  {{template "foot" .}}
{{end}}

{{block "graphs" .}}
  <h2>Graphs</h2>

  <button hx-get="/graphs"
          hx-target="next .all-graphs">Refresh</button>

  <div class="all-graphs">
    {{template "all-graphs" .}}
  </div>
{{end}}

{{block "all-graphs" .}}
  <p>Rendered: {{.Servertime}} (duration {{.RenderingTime}})</p>
  {{range .Graphs}}
    {{.Element}}
    {{.Script}}
  {{end}}
{{end}}
